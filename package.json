{
  "name": "blog",
  "description": "Personal blog",
  "scripts": {
    "dev": "npx pequeno --serve --noAfterBuild --noProcessHtml",
    "dev-blog": "npx pequeno --serve --page=blog --path=/blog/index.html --data=posts --noAfterBuild --noProcessHtml",
    "dev-post": "npx pequeno --serve --page=post-item --path=/scialpinismo/2022/03/20/monte-sissone-dal-passo-del-maloja/index.html --data=posts --noAfterBuild --noProcessHtml",
    "dev-portfolio": "npx pequeno --serve --page=portfolio --data=portfolio --noAfterBuild --noProcessHtml",
    "dev-contatti": "npx pequeno --serve --page=contatti --noData --noAfterBuild --noProcessHtml",
    "dev-grazie": "npx pequeno --serve --page=grazie --noData --noAfterBuild --noProcessHtml --noCopy --noPublicCopy",
    "dev-index": "npx pequeno --serve --page=index --data=posts --noAfterBuild --noProcessHtml --noCopy --noPublicCopy",
    "build": "npx pequeno --clean",
    "build-verbose": "npx pequeno --clean --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signalkuppe/blog.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signalkuppe/blog/issues"
  },
  "homepage": "https://www.signalkuppe.com",
  "devDependencies": {
    "@contentful/rich-text-html-renderer": "^14.1.2",
    "contentful": "^8.4.0",
    "dayjs": "^1.10.6",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "pequeno": "0.1.2",
    "sitemap": "^7.0.0"
  },
  "dependencies": {
    "@tarekraafat/autocomplete.js": "^10.2.6",
    "chart.js": "^3.4.1",
    "glightbox": "^3.2.0",
    "leaflet": "^1.7.1",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-gesture-handling": "^1.2.1",
    "leaflet-gpx": "^1.5.2",
    "scrollama": "^2.2.2",
    "tippy.js": "^6.3.2"
  }
}
