{
  "name": "blog",
  "description": "Personal blog",
  "scripts": {
    "dev": "npm run build-cerca && npm run copy-cerca-assets && npx pequeno --serve --noAfterBuild",
    "dev-prod": "npm run build-cerca && npm run copy-cerca-assets && SIGNALKUPPE_WEBSITE_ENV=production npx pequeno --serve",
    "dev-blog": "npx pequeno --serve --page=blog --path=/blog/index.html --data=posts --noAfterBuild",
    "dev-blog-category": "npx pequeno --serve --page=blog-by-category --path=/blog/alpinismo/index.html --data=posts --noAfterBuild",
    "dev-post": "npx pequeno --serve --page=post-item --path=/alpinismo/2023/07/10/monte-bianco-via-italiana-dal-rifugio-gonella/index.html --data=posts --dataParam=4yo2futMEtbEjgH3MrRvN4 --noAfterBuild",
    "dev-post-prod": "SIGNALKUPPE_WEBSITE_ENV=production npx pequeno --serve --page=post-item --path=/alpinismo/2023/07/10/monte-bianco-via-italiana-dal-rifugio-gonella/index.html --data=posts --dataParam=4yo2futMEtbEjgH3MrRvN4 --noAfterBuild",
    "dev-post-item": "npx pequeno --serve --page=post-item --data=posts --noAfterBuild",
    "dev-post-photo": "npx pequeno --serve --page=post-photo --path=/scialpinismo/2022/03/03/norvegia-lofoten/tramonto-a-svolvaer-avoelpimvfl0i39stnahp/index.html --data=posts --noAfterBuild",
    "dev-portfolio": "npx pequeno --serve --page=portfolio --data=portfolio --noAfterBuild",
    "dev-portfolio-photo": "npx pequeno --serve --page=portfolio-photo --path=/portfolio/discesa-verso-il-varrone-delle-vacche-6f1tvpogzqkvdsiwf9okgu/index.html --data=portfolio --dataParam=6f1TvpogzQkvdSIWF9okgu --noAfterBuild",
    "dev-contatti": "npx pequeno --serve --page=contatti --noData --noAfterBuild",
    "dev-grazie": "npx pequeno --serve --page=grazie --noData --noAfterBuild",
    "dev-index": "npx pequeno --serve --page=index --noData --noAfterBuild",
    "dev-404": "npx pequeno --serve --page=404 --noData --noAfterBuild",
    "dev-rss": "npx pequeno --page=post-item --data=posts",
    "dev-jsOptimization": "npx pequeno --serve --page=post-item --path=/alpinismo/2023/07/10/monte-bianco-via-italiana-dal-rifugio-gonella/index.html --data=posts",
    "dev-cerca": "npm run build-blog && npm run copy-posts-json && cd src/pages/cerca && vite",
    "dev-meteo": "npx netlify functions:serve --port=3000 & cd src/pages/meteo && vite",
    "copy-posts-json": "mkdir -p ./src/pages/cerca/_data && cp .site/_data/posts.json ./src/pages/cerca/_data/posts.json",
    "copy-cerca-assets": "mkdir -p .site/assets && cp -R .site/cerca/assets .site",
    "build-cerca": "cd src/pages/cerca && vite build",
    "build": "npx pequeno --clean && npm run build-cerca && npm run copy-cerca-assets",
    "build-verbose": "npx pequeno --clean --verbose",
    "build-blog": "npx pequeno --clean --page=blog --path=/blog/index.html --data=posts",
    "lint:js": "eslint --ext .jsx --ext .js .",
    "lint:css": "stylelint **/*.{js,jsx}",
    "lint": "npm run lint:js && npm run lint:css",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signalkuppe/blog.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signalkuppe/blog/issues"
  },
  "homepage": "https://www.signalkuppe.com",
  "devDependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.0.4",
    "clean-css": "^5.3.2",
    "contentful": "^10.4.2",
    "dayjs": "^1.11.9",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-webpack": "^0.13.4",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "husky": "^8.0.3",
    "jstoxml": "^3.2.7",
    "netlify-cli": "^16.4.2",
    "pequeno": "0.2.1",
    "postcss-styled": "^0.34.0",
    "postcss-styled-syntax": "^0.4.0",
    "sitemap": "^7.1.1",
    "slug": "^8.2.3",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "uglify-js": "^3.17.4",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0"
  },
  "dependencies": {
    "@nivo/core": "^0.83.0",
    "@nivo/line": "^0.83.0",
    "chart.js": "^4.3.3",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-gpx": "^1.7.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "react-query": "^3.39.3",
    "rooks": "^7.14.1",
    "scrollama": "^3.2.0"
  }
}
